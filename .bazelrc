build --incompatible_enable_cc_toolchain_resolution
build --action_env BAZEL_DO_NOT_DETECT_CPP_TOOLCHAIN=1
build --spawn_strategy=worker,processwrapper-sandbox
build --per_file_copt='external/zlib[~/].*\.c@-std=c90' --host_per_file_copt='external/zlib[~/].*\.c@-std=c90

build:aarch64_cross --platforms=//platforms:aarch64

test --test_output=all --keep_going --color=yes
